	spi@f9923000 {
		status = "disabled";
	};

	spi@f9966000 {
		status = "disabled";
	};

	qcom,mdss_dsi@fd922800 {
		qcom,dsi-pref-prim-pan = <&dsi_sharp_1080_vid>; // Add for ASUS Display driver 
		qcom,dsi-pref-prim-pan = <&dsi_nt35595_1080_cmd>; // Add for ASUS Innolux command mode driver 
        	qcom,dsi-pref-prim-pan = <&dsi_nt35595_1080_video>;// Add for ASUS Innolux video mode driver 
		qcom,platform-reset-gpio = <&msmgpio 47 1>;
	};

	qcom,mdss_mdp@fd900000 {
		qcom,mdss-pref-prim-intf = "dsi";
	};

	qcom,mdss_dsi_orise_720p_video {
		status = "disable";
	};

	sound {
		qcom,model = "msm8974-taiko-cdp-snd-card";
		qcom,hdmi-audio-rx;
		qcom,us-euro-gpios = <&pm8941_gpios 20 0>;
		qcom,cdc-micbias2-headset-only;
		qcom,hsmic_bias = <&pm8941_gpios 23 0>;

		button {
			label = "button";
			gpios = <&msmgpio 80 0x00>;
			interrupt-parent = <&msmgpio>;
			interrupts = <80 0x2>;
		};
		jack {
			label = "jack";
			gpios = <&msmgpio 1 0x00>;
			interrupt-parent = <&msmgpio>;
			interrupts = <1 0x2>;
		};
		hs_path_en {
			label = "hs_path_en";
			gpios = <&msmgpio 13 0x00>;
		};
	};

	usb2_otg_sw: regulator-tpd4s214 {
		compatible = "regulator-fixed";
		regulator-name = "usb2_otg_sw";
		gpio = <&pm8941_gpios 18 0>;
		parent-supply = <&pm8941_boost>;
		startup-delay-us = <17000>;
		enable-active-high;
	};

	hsic_host: hsic@f9a00000 {
		compatible = "qcom,hsic-host";
		reg = <0xf9a00000 0x400>;
		#address-cells = <0>;
		interrupt-parent = <&hsic_host>;
		interrupts = <0 1 2>;
		#interrupt-cells = <1>;
		interrupt-map-mask = <0xffffffff>;
		interrupt-map = <0 &intc 0 136 0
			1 &intc 0 148 0
			2 &msmgpio 144 0x8>;
		interrupt-names = "core_irq", "async_irq", "wakeup";
		hsic_vdd_dig-supply = <&pm8841_s2_corner>;
		HSIC_GDSC-supply = <&gdsc_usb_hsic>;
		hsic,strobe-gpio = <&msmgpio 144 0x00>;
		hsic,data-gpio = <&msmgpio 145 0x00>;
		hsic,resume-gpio = <&msmgpio 80 0x00>;
		hsic,ignore-cal-pad-config;
		hsic,strobe-pad-offset = <0x2050>;
		hsic,data-pad-offset = <0x2054>;
		qcom,phy-susp-sof-workaround;
		hsic,vdd-voltage-level = <1 5 7>;

		qcom,msm-bus,name = "hsic";
		qcom,msm-bus,num-cases = <2>;
		qcom,msm-bus,num-paths = <1>;
		qcom,msm-bus,vectors-KBps =
				<85 512 0 0>,
				<85 512 40000 160000>;
        };

	wlan0: qca,wlan {
		compatible = "qca,ar6004-hsic";
		qcom,msm-bus,name = "wlan";
		qcom,msm-bus,num-cases = <5>;
		qcom,msm-bus,active-only = <0>;
		qcom,msm-bus,num-paths = <1>;
		qcom,msm-bus,vectors-KBps =
				<85 512 0 0>,
				<85 512 40000 160000>,
				<85 512 40000 320000>,
				<85 512 40000 480000>,
				<85 512 40000 800000>;
	};

	gen-vkeys {
		compatible = "qcom,gen-vkeys";
		label = "atmel_mxt_ts";
		qcom,disp-maxx = <720>;
		qcom,disp-maxy = <1280>;
		qcom,panel-maxx = <760>;
		qcom,panel-maxy = <1424>;
		qcom,key-codes = <158 139 102 217>;
	};

	gpio_keys {
		compatible = "gpio-keys";
		input-name = "gpio-keys";

		vol_up {
			label = "volume_up";
			gpios = <&msmgpio 92 0x00>;
			interrupt-parent = <&msmgpio>;
			interrupts = <92 0x2>; 
			linux,input-type = <1>;
			linux,code = <115>;
			gpio-key,wakeup;
			debounce-interval = <1>;
		};

		vol_down {
			label = "volume_down";
			gpios = <&msmgpio 95 0>;
			interrupt-parent = <&msmgpio>;
			interrupts = <95 0x2>;
			linux,input-type = <1>;
			linux,code = <114>;
			gpio-key,wakeup;
			debounce-interval = <1>;
		};

	        pwr_key {
	            label = "pwr_key";
	            gpios = <&msmgpio 28 0x00>;
	            interrupt-parent = <&msmgpio>;
	            interrupts = <28 0x2>;
	            linux,input-type = <1>;
	            linux,code = <116>;
	            gpio-key,wakeup;
	            debounce-interval = <1>;
	        };
	};

	//ASUS BSP Jacob kung +++
	backlight {
		compatible = "qcom,A86-backlight";
		input-name = "backlight";
		BL_5V-supply = <&pm8941_boost>;
		pan-backlight-levels = <1 255>;
		bl-tout-defau-levels = <7 58>;
		normod-bl-param = <17 151 412>;
		outmod-bl-param = <58 255 340>;
		autmod-bl-param = <17 255 272>;
		factory-bl-param = <0 255 272>;
		pad-normod-bl-param = <0 250 240>;
		pad-outmod-bl-param = <33 255 557>;
		pad-autmod-bl-param = <9 255 448>;
		status = "okay";
	};
	//ASUS BSP Jacob kung +++

//ASUS BSP LiJen +++
	spi_9: spi@f9965000 { /* BLSP2 QUP2 */
		compatible = "qcom,spi-qup-v2";
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "spi_physical", "spi_bam_physical";
		reg = <0xf9965000 0x1000>,
		      <0xf9944000 0x19000>;
		interrupt-names = "spi_irq", "spi_bam_irq";
		interrupts = <0 103 0>, <0 239 0>;

//		reg-names = "spi_physical";
//		reg = <0xf9965000 0x1000>;
//		interrupt-names = "spi_irq";
//		interrupts = <0 103 0>;

		spi-max-frequency = <50000000>;
		qcom,gpio-mosi = <&msmgpio 49 0>;
		qcom,gpio-miso = <&msmgpio 50 0>;
		qcom,gpio-clk  = <&msmgpio 52 0>;
		qcom,gpio-cs0  = <&msmgpio 51 0>;

		spi_cpol;
		spi_cpha;
		
		qcom,infinite-mode = <0>;
		qcom,use-bam;
		qcom,ver-reg-exists;
		qcom,bam-consumer-pipe-index = <16>;
		qcom,bam-producer-pipe-index = <17>;
		qcom,master-id = <84>;
		qcom-spi-test@0{
			compatible = "qcom,spi-test";
			reg = <0>;
			spi-max-frequency = <50000000>;
			qcom,use-bam;
			qcom,bam-consumer-pipe-index = <16>;
			qcom,bam-producer-pipe-index = <17>;

			//interrupt-parent = <&msmgpio>;
 		};
	};

