&soc {
	qcom,hdmi_tx@fd922100 {
		status = "ok";
	};

	slim@fe12f000 {
		taiko_codec {
			qcom,cdc-dmic-sample-rate = <2400000>;
		};
	};

	serial@f991e000 {
		status = "ok";
	};

	i2c@f9923000 {
		status = "ok";
	};

	i2c@f9967000 {
		status = "disabled";
	};

	i2c@f9966000 { /*BLSP#10 */
		cell-index = <10>;
		compatible = "qcom,i2c-qup";
		reg = <0xf9966000 0x500>;
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr";
		interrupts = <0 104 0>;
		interrupt-names = "qup_err_intr";
		qcom,i2c-bus-freq = <100000>;
		qcom,i2c-src-freq = <19200000>;
		qcom,clk-ctl-xfer;
		qcom,master-id = <84>;
		qcom,active-only;
		qcom,use-bam;
		qcom,ver-reg-exists;
	};

	i2c@f9968000 {
		cell-index = <12>;
		compatible = "qcom,i2c-qup";
		reg = <0xf9968000 0x500>;
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr";
		interrupts = <0 106 0>;
		interrupt-names = "qup_err_intr";
		qcom,i2c-bus-freq = <100000>;
		qcom,i2c-src-freq = <19200000>;
