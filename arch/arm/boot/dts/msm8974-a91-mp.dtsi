// +++.....................................................I2C bus part....................................................+++//
//+++ ASUS BSP Eason......."BLSP1 : Wireless charger"
	i2c@f9923000 {
		IDT_P9023_i2c@39 {
			compatible = "IDT,P9023";
			reg = <0x39>;
			interrupt-parent = <&msmgpio>;
		};
	};
//--- ASUS BSP Eason......."BLSP1 : Wireless charger"


// +++ ASUS_BSP : add for ASUS touch 
	i2c@f9924000 {
		himax_hx8528_d48@48 {
			compatible = "himax,himax-ts";
			reg = <0x48>;
			interrupt-parent = <&msmgpio>;
			interrupts = <0 0x2>;
			vdd_ana-supply = <&pm8941_l23>;
			vcc_i2c-supply = <&pm8941_lvs1>;
			himax,reset-gpio = <&msmgpio 60 0x00>;
			himax,irq-gpio = <&msmgpio 0 0x00>;
			himax,pcb-id0 = <&msmgpio 69 0x00>;
			himax,pcb-id1 = <&msmgpio 90 0x00>;
		};
	};
// --- ASUS_BSP : add for ASUS touch 

// +++ ASUS BSP Bernard Ong: mydp 7808
	i2c@f9925000{
		colorado_i2c@39 {
			compatible = "analogix,slimport";
			reg = <0x39>;
			interrupt-parent = <&msmgpio>;
			#interrupt-cells = <2>;
			interrupts = <82 0x2 61 0x2>;
			vcc_1v-supply = <&pm8941_l3>;
			slimport,gpio_rst = <&msmgpio 45 0x00>; //Reset, MYDP_RESETN_N
			slimport,gpio_cbl_det = <&msmgpio 61 0x00>; //cable detect, MYDP_CABLE_DET
			slimport,gpio_pw_dwn = <&msmgpio 31 0x00>; //chip power down, MYDP_EN_N
			slimport,gpio_usb_id = <&msmgpio 82 0x00>; //usb_hs_id
			slimport,hdmi-tx-map = <&mdss_hdmi_tx>;  //ASUS BSP Wei +++
		};
	};
//--- ASUS BSP Bernard Ong: mydp 7808

//+++ ASUS BSP......."BLSP6 : Pad bus"
